{
  "name": "SNN Clustering",
  "description": "Performs SNN graph-based clustering and return group membership.",
  "tags": [
    "omics",
    "sc-RNA-seq",
    "clustering",
    "machine learning"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/snn_clustering_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "k",
      "defaultValue": 20,
      "description": "Number of nearest neighbors to use in clustering"
    },
    {
      "kind": "DoubleProperty",
      "name": "snn_eps",
      "defaultValue": 0.8,
      "description": "Two objects are only reachable from each other if they share at least eps nearest neighbors."
    },
    {
      "kind": "DoubleProperty",
      "name": "snn_minPts",
      "defaultValue": 16,
      "description": "Minimum number of points that share at least eps nearest neighbors for a point to be considered a core point."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "cell ID",
            "type": "string",
            "description": "The cell identifiers",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "principal component name",
            "type": "string",
            "description": "Principal component identifiers",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "principal component value",
                "type": "numeric",
                "description": "Principal component values",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}