{
  "name": "SNN Clustering",
  "description": "Performs SNN graph-based clustering and return group membership.",
  "tags": [
    "omics",
    "sc-RNA-seq",
    "clustering",
    "machine learning"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/snn_clustering_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "k",
      "defaultValue": 20,
      "description": "Number of neighbours to use in clustering"
    },
    {
      "kind": "DoubleProperty",
      "name": "snn_eps",
      "defaultValue": 0.8,
      "description": "Two objects are only reachable from each other if they share at least eps nearest neighbors."
    },
    {
      "kind": "DoubleProperty",
      "name": "snn_minPts",
      "defaultValue": 16,
      "description": "Minimum number of points that share at least eps nearest neighbors for a point to be considered a core points."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers (features)",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observation",
            "type": "",
            "description": "Observation identifiers (cells, samples)",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}
